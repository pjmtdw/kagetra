{
  "parser": "vue-eslint-parser",
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended"
  ],
  "globals": {
    "axios": true,
    "_": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-plusplus": [
      "error",
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "max-len": 1,
    "vue/max-attributes-per-line": [1, {
      "singleline": 10,
      "multiline": {
        "max": 10,
        "allowFirstLine": true
      }
    }],
    "vue/require-default-prop": 1,
    "vue/require-v-for-key": 1
  },
  "env": {
    "browser": true,
    "jquery": true
  }
}
