{
  "root": true,
  "parser": "vue-eslint-parser",
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": [
    "plugin:vue/recommended",
    "@vue/airbnb"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "globals": {
    "axios": true,
    "_": true
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "MemberExpression": "off"
      }
    ],
    "no-plusplus": [
      "error",
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "max-len": [
      "warn",
      {
        "code": 120,
        "ignoreTemplateLiterals": true,
        "ignorePattern": "^\\s+<|>$"
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true
      }
    ],
    "no-multi-spaces": [
      "error",
      {
        "ignoreEOLComments": true
      }
    ],
    "no-else-return": [
      "error",
      {
        "allowElseIf": true
      }
    ],
    "operator-linebreak": [
      "error",
      "after",
      {
        "overrides": {
          "?": "before"
        }
      }
    ],
    "no-restricted-globals": 0,
    "object-curly-newline": [
      "error",
      {
        "consistent": true
      }
    ],
    "prefer-destructuring": [
      "error",
      {
        "array": false,
        "object": true
      }
    ],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "vue/max-attributes-per-line": [
      1,
      {
        "singleline": 10,
        "multiline": {
          "max": 10,
          "allowFirstLine": true
        }
      }
    ],
    "vue/require-default-prop": 1,
    "vue/require-v-for-key": 1,
    "vue/attributes-order": 0,
    "vue/html-closing-bracket-spacing": 0,
    "vue/html-closing-bracket-newline": [
      "error",
      {
        "singleline": "never",
        "multiline": "never"
      }
    ],
    "vue/no-v-html": 1
  }
}
